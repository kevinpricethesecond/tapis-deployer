#!/bin/bash

echo "burnup authenticator:"

export POSTGRES_PASSWORD=`grep DBCREDENTIAL_POSTGRES_POSTGRES_AUTHENTICATOR_AUTHENTICATOR_PASSWORD {{tapisdatadir}}/skadmin/env | awk -F= '{print $2}' | tr -d " \t\n\r"`
export postgres_password=`grep DBCREDENTIAL_POSTGRES_POSTGRES_AUTHENTICATOR_AUTHENTICATOR_PASSWORD {{tapisdatadir}}/skadmin/env | awk -F= '{print $2}' | tr -d " \t\n\r"`
export LDAP_ROOTPASS=`grep SERVICEPWD_AUTHENTICATOR_PASSWORD {{tapisdatadir}}/skadmin/env | awk -F= '{print $2}' | tr -d " \t\n\r"`
export service_password=`grep SERVICEPWD_AUTHENTICATOR_PASSWORD {{tapisdatadir}}/skadmin/env | awk -F= '{print $2}' | tr -d " \t\n\r"`

docker compose up -d

